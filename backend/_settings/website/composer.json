        "dmstr/yii2-adminlte-asset": "^2.1",
        "kartik-v/yii2-widget-fileinput": "dev-master",
        "dompdf/dompdf": "^0.8.3",
        "kartik-v/bootstrap-popover-x": "^1.4",
        "kartik-v/yii2-popover-x": "dev-master",
        "kartik-v/yii2-grid": "dev-master",
        "kartik-v/yii2-widgets": "@dev",
        "kartik-v/yii2-editable": "@dev",
        "kartik-v/yii2-checkbox-x": "@dev",
        "kartik-v/yii2-date-range": "@dev",
        "kartik-v/yii2-money": "@dev",
        "kartik-v/yii2-number": "@dev",
        "kartik-v/yii2-slider": "@dev",
        "kartik-v/yii2-sortable-input": "dev-master",
        "kartik-v/yii2-bootstrap4-dropdown": "@dev",
        "leandrogehlen/yii2-treegrid": "*",
        "rmrevin/yii2-fontawesome": "~3.4",
        "bower-asset/jquery.switcher": "^1.2",
        "2amigos/yii2-ckeditor-widget": "~1.0",
        "newerton/yii2-fancybox-3": "*",
        "npm-asset/popper.js": "^1.14.3",
        "npm-asset/font-awesome": "^4.7.0",
        "npm-asset/jquery": "^3.4.1",
        "urands/firephp": "^1.0",
        "yiisoft/yii2-jui": "~2.0.0",
        "conquer/codemirror": "*",
        "himiklab/yii2-recaptcha-widget": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "4.0.x-dev | ^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset":{
            "installer-paths": {
                "npm-asset-library": "vendor/npm-asset",
                "bower-asset-library": "vendor/bower-asset"
            }
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
